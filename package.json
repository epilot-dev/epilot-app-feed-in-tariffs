{
	"name": "epilot-app-feed-in-tariffs",
	"version": "1.0.0",
	"description": "This app fetches feed-in tariff prices from data provided on www.netztransparenz.de",
	"main": "index.js",
	"scripts": {
		"test": "vitest",
		"test:run": "vitest run",
		"test:integration": "INTEGRATION_TESTS=true npx sst shell vitest run",
		"populate": "npx sst shell tsx scripts/populate-data.ts",
		"deploy": "sst deploy --stage production",
		"dev": "sst dev",
		"format": "biome format --write .",
		"format:check": "biome format .",
		"lint": "biome check .",
		"lint:fix": "biome check --write ."
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"dependencies": {
		"@aws-sdk/client-dynamodb": "^3.890.0",
		"@aws-sdk/lib-dynamodb": "^3.890.0",
		"@epilot/entity-client": "^6.9.0",
		"sst": "3.17.13",
		"tsx": "^4.20.5",
		"xlsx": "^0.18.5"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.2.4",
		"@types/aws-lambda": "8.10.152",
		"@types/node": "^24.5.1",
		"vitest": "^3.2.4"
	}
}
